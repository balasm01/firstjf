node {
    stage('git-pull'){
        echo 'pulling required branch'
        git branch : '${branch}', url: 'https://github.com/balasm01/jenkinsbuild.git'
    }
    stage('build'){
        echo 'Building file'
        sh 'java -version'
    }
    stage('test'){
        echo 'Testing file'
    }
    stage('deploy'){
        echo 'Deploying file'
    }
    stage('final'){
        echo 'final file version 1.0'
    }     
}

